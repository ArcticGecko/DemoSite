{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": false,
    "removeComments": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "target": "esnext",
    "jsx": "react",
    "baseUrl": ".",
    "outDir": "./tsOutput",
    "allowJs": true,
    "paths": {
      "*": [
        "Scripts/@types/*"
      ]
    },
    "typeRoots": [
      "Scripts/@types",
      "node_modules/@types"
    ],
    "rootDir": "./Scripts"
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "./tsOutput",
    "./watcher.js",
    "./webpack.config.js"
  ]
}
